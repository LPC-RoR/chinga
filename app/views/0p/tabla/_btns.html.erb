<% if a == 'seleccion' %>
	<%= render partial: '0p/tabla/btn_seleccion', locals: {link_seleccion: "#{@link_seleccion}#{objeto.id}" } %>
<% else %>
	<% if btns?(objeto, 'crud') %>
		<%= render partial: '0p/tabla/btns_e', locals: {objeto: objeto} %>
		<!-- BOTONES ESTADO : SOLO -->
		<%= render partial: '0p/tabla/btns_estado', locals: {objeto: objeto, estados: objeto.class.name.constantize::ESTADOS, link_estado: "/#{objeto.class.name.tableize}/#{objeto.id}/estado?estado="} if in_t?(objeto.class.name.tableize, :estados) %>
	<% end %>

	<% if btns?(objeto, 'x') %>
		<% x_btns(objeto).each do |btn| %>
			<%= render partial: '0p/general/btn_w_html_options', locals: {texto: btn[0], link: "#{btn[1]}#{objeto.id}#{btn[2]}#{(!!btn[2].match(/\?+/) ? '&' : '?') if btn[3]}#{"objeto_id=#{@objeto.id if @objeto.present?}" if btn[3]}", delete: false} if x_conditions(objeto, btn[0]) %>
		<% end %>
	<% end %>
<% end %>