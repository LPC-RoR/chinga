<p id="notice"><%= notice %></p>

<h1>Ultimas</h1>

<table>
  <thead>
    <tr>
      <th>Ultima</th>
      <th>Sha1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ultimas.each do |ultima| %>
      <tr>
        <td><%= ultima.ultima %></td>
        <td><%= ultima.sha1 %></td>
        <td><%= link_to 'Show', ultima %></td>
        <td><%= link_to 'Edit', edit_ultima_path(ultima) %></td>
        <td><%= link_to 'Destroy', ultima, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ultima', new_ultima_path %>
