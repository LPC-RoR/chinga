<p id="notice"><%= notice %></p>

<h1>Soportes</h1>

<table>
  <thead>
    <tr>
      <th>Soporte</th>
      <th>Tipo</th>
      <th>Anio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @soportes.each do |soporte| %>
      <tr>
        <td><%= soporte.soporte %></td>
        <td><%= soporte.tipo %></td>
        <td><%= soporte.anio %></td>
        <td><%= link_to 'Show', soporte %></td>
        <td><%= link_to 'Edit', edit_soporte_path(soporte) %></td>
        <td><%= link_to 'Destroy', soporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Soporte', new_soporte_path %>
