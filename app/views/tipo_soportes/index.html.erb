<p id="notice"><%= notice %></p>

<h1>Tipo Soportes</h1>

<table>
  <thead>
    <tr>
      <th>Tipo soporte</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_soportes.each do |tipo_soporte| %>
      <tr>
        <td><%= tipo_soporte.tipo_soporte %></td>
        <td><%= link_to 'Show', tipo_soporte %></td>
        <td><%= link_to 'Edit', edit_tipo_soporte_path(tipo_soporte) %></td>
        <td><%= link_to 'Destroy', tipo_soporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Soporte', new_tipo_soporte_path %>
