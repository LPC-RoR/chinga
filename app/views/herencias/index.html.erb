<p id="notice"><%= notice %></p>

<h1>Herencias</h1>

<table>
  <thead>
    <tr>
      <th>Equipo</th>
      <th>Carpeta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @herencias.each do |herencia| %>
      <tr>
        <td><%= herencia.equipo_id %></td>
        <td><%= herencia.carpeta_id %></td>
        <td><%= link_to 'Show', herencia %></td>
        <td><%= link_to 'Edit', edit_herencia_path(herencia) %></td>
        <td><%= link_to 'Destroy', herencia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Herencia', new_herencia_path %>
